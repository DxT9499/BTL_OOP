<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="418.0" prefWidth="768.0"
            xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/2.2"
            fx:controller="sample.Controllers.AddController">
    <children>
        <Label layoutX="98.0" layoutY="30.0" prefHeight="60.0"
               prefWidth="125.0" text="Word: " />
        <TextField fx:id="word" layoutX="267.0" layoutY="44.0"
                   prefHeight="32.0" prefWidth="322.0" />
        <Label layoutX="98.0" layoutY="101.0" prefHeight="60.0"
               prefWidth="125.0" text="Pronunciation:" />
        <TextField fx:id="pronounce" layoutX="267.0" layoutY="115.0"
                   prefHeight="32.0" prefWidth="322.0" />
        <Label layoutX="98.0" layoutY="170.0" prefHeight="60.0"
               prefWidth="111.0" text="Meaning:" />
        <TextField fx:id="meaning" layoutX="267.0" layoutY="184.0"
                   prefHeight="32.0" prefWidth="322.0" />
        <Button layoutX="427.0" layoutY="267.0" mnemonicParsing="false"
                onAction="#addWord" prefHeight="32.0" prefWidth="162.0"
                text="Add" />
        <Label fx:id="info" layoutX="209.0" layoutY="339.0"
               prefWidth="-1.0" text="" textFill="RED" />
    </children>
</AnchorPane>
